{
  "multipart": [
    { "apply": { "model": "simplycats:bowls/cat_bowl_blue" } },

    { "when": { "full_food": "true" },
      "apply": { "model": "simplycats:bowls/cat_bowl_full_food" }
    },

    { "when": { "water_level": "3|2|1" },
      "apply": { "model": "simplycats:bowls/cat_bowl_full" }
    }
  ]
}
